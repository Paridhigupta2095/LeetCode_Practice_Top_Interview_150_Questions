class Solution:
    def isValid(self, s: str) -> bool:
        
        stack=[]
        
        if len(s)%2!=0:
            return False

        for i in range(0,len(s)):

            if (len(stack)!=0 and ((stack[-1]=='(' and s[i] ==')') or (stack[-1]=='{' and s[i] =='}') or (stack[-1]=='[' and s[i] ==']'))):
                stack.pop()
            else:
                stack.append(s[i])
            i+=1   
        
        if len(stack)==0:
            return True
        else:
            return False


# -------- Driver Code --------
if __name__ == "__main__":
    sol = Solution()
    
    test_cases = ["()", "()[]{}", "(]", "([)]", "{[]}", "(((("]
    
    for s in test_cases:
        print(f"{s} -> {sol.isValid(s)}")
